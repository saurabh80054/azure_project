{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf0project1"
		},
		"adls_project1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_project1'"
		},
		"sql_project1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sql_project1'"
		},
		"adls_project1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storage0project1.dfs.core.windows.net/"
		},
		"sql_project1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver0project1.database.windows.net"
		},
		"sql_project1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "database0project1"
		},
		"sql_project1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminSaurabh"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf0project1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_project1_accountKey": {
						"value": "[parameters('adls_project1_accountKey')]"
					},
					"sql_project1_password": {
						"value": "[parameters('sql_project1_password')]"
					},
					"adls_project1_properties_typeProperties_url": {
						"value": "[parameters('adls_project1_properties_typeProperties_url')]"
					},
					"sql_project1_properties_typeProperties_server": {
						"value": "[parameters('sql_project1_properties_typeProperties_server')]"
					},
					"sql_project1_properties_typeProperties_database": {
						"value": "[parameters('sql_project1_properties_typeProperties_database')]"
					},
					"sql_project1_properties_typeProperties_userName": {
						"value": "[parameters('sql_project1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}